html, body {
    width: 100%;
    height: 100%;
    margin: 0px;
    padding: 0px;
    overflow: hidden;
}
.nes {
	display:inline-block;
	height:30px;
    color: red;
    font-size: 14px;
    line-height: 30px;
	width:4px;
	margin: 0 4px;
}
#body-panel {
    position: absolute;
    top: 0px; left: 0px; right: 0px; bottom: 0px;
    _width: 100%;
    _height: 100%;
    overflow: auto;
    z-index: 1;
	margin-top:28px;
}
#float-panel{
	visibility:hidden;
    top: 0px; left: 0px;
	position:relative;
	overflow:hidden;
	z-index:2;
}
#menu-panel{
	margin:10px 0 10px 380px;
	width: 250px;
	padding: 10px;
	background-color:#F5FCFE;
}
#icon-panel{
	text-align:center;
	padding:4px;
	background-color:#fff;
	border:1px solid #999;
	position: fixed;
	top:40px; left:10px;
	width:568px;
	z-index:2;
}
#icon-panel .iconp-buttons{
	border-top:1px solid #999;
}
#help-info{
	margin-top:10px;
	padding:10px;
	text-align:left;
	font-size:12px;
	border-top:1px solid #99ccff;
	color:#999;
}
#help-info dd{
	margin-left:20px;
}
#menu-manage-panel{
	border:1px solid #99ccff;
	text-align:center;
	visibility:visible;
	background-color:#F5FCFE;
	position:fixed;
	top:40px; left:10px;
	width:350px;
	z-index:1;
}
#menu-manage-panel h4{
	margin:10px;
	font-size:14px;
	border-bottom: 1px solid#c8d8e5;
	line-height:32px;
	height:32px;
	background:url(image/kuangbg.jpg);
}
#menu-manage-panel form label{
	font-size:12px;
	display:inline-block;
}
#menu-manage-panel form input[type=text],#menu-manage-panel form select{
	border:1px solid #999999 ;
	background:url(../../images/public/inputText_bg.jpg) repeat-x;
	height:22px;
	line-height:23px;
	width:180px;
	display:inline-block;
}
#menu-manage-panel table{
	margin:auto;
}
#menu-manage-panel th{
	padding-right:10px;
	text-align: right;
}
#menu-manage-panel td{
	text-align: left;
}
#nav-bar{
	visibility:visible;
	padding-left:10px;
	background:url(../../images/public/daohangbg.jpg) repeat-x;
	line-height:28px;
	height:28px;
	font-size:12px;
}
#nav-bar a{
	color:#000;
	text-decoration:none;
	float:right;
}
#nav-buttons{
	visibility:visible;
	background:url(../../images/public/buttonsbg.jpg) repeat-x;
	border-bottom: 1px solid #CCC;
	height: 43px;
}
.tsd-button, #nav-buttons button {
    background: url(../../images/public/buttonbg.jpg) repeat-x;
    border: 1px solid #CCC;
    cursor: pointer;
    height: 28px;
    margin-left: 5px;
    margin-top: 10px;
    padding: 4px 8px;
}
#oper-T {			
	z-index:3;
	top:150px; 
	left:250px;
	padding:0px;
	width:800px;
	position: fixed;
}
	