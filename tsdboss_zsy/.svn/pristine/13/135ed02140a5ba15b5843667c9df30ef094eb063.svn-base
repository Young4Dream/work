*{
	padding:0;
	margin:0;
	}
body{
	font-family: verdana;
	width:100%;
	}	
	/*header*/
.header_bk{
	width:100%;
	height:70px;
	padding-top:5px;
	background: #f7f7f7;
	}	
.header{
	width:800px;
	height:70px;
	display:block;
	/*position:relative;*/
	margin: 0 auto;
	z-index: 2;
	
	overflow: hidden;
	}
a {
	text-decoration: none;
	color:#5FB0D5;
	
	padding:0px 3px;
}
a:hover {
	color: #F96;
}
select,input,button,button img{
	vertical-align: middle
}
ul {
	list-style: none;
	vertical-align:middle;
}
ul li{
	float:right;
	color:#5FB0D5;
	font-size:12px;
	font-weight:bold;
	
	}
.logo{
	background:url(../../images/skin_0/login_pic/logo.png) no-repeat;
	width:84px;
	height:70px;
	float:left;
	}
.header_word{
	width:466px;
	height:70px;
	line-height:70px;
	font-size:24px;
	color:#5FB0D5;
	font-weight:bold;
	float:left;
	font-family:Arial, Helvetica, sans-serif;
	}
.header_right_word{
	width:250px;
	height:70px;
	line-height:70px;
	float:left;
	}	
.main_out{
	width:100%;
	height:440px;
	/*background:#AEAC55;*/
	/*filter:alpha(opacity=100);*/
	}	
.main{
	width:308px;
	height:358px;
	float:right;
	margin-right:100px;
	margin-top:40px;
	background:#FBFAFA;
	border:1px solid #667C93;
	filter:alpha(opacity=90);
	}
.top_out{
	width:310px;
	height:40px;
	
	}	
.top_line{
	width:308px;
	background:#D0E0F7;
	/*background:url(../../images/skin_0/login_pic/line_bg.gif) repeat-x;*/
	border-bottom:1px solid #667C93;
	/*border-left:1px solid #667C93;*/
	height:40px;
	/*display:block;统一转化为块级元素
	overflow: hidden; 
	width: 0; 
	height: 0;*/ 
	}	
.top_line select{
	width:140px;
	height:30px;
	padding:3px;
	font-size:16px;
	float:right;
	margin-right:2px;
	margin-top:5px;
	font-size:13px;
	line-height:30px;
	}

.input_div{
	width:308px;
	height:40px;
	float:right;
	line-height:30px;
	text-align:center;

	}
.input_div2{
	width:308px;
	height:30px;
	margin-top:15px;
	float:right;
	line-height:30px;
	text-align:center;

	}	
input{
	height:25px;
	width:180px;
	line-height:25px;
	word-spacing:1px;
	letter-spacing:1px;
	vertical-align:middle;
	font-size:15px;
	font-weight:bold;
	}	
	
.anniu{
	width:308px;
	height:30px;
	margin-top:65px;
	float:right;
	line-height:40px;
	text-align:center;
	}	
.kong{
	width:53px;
	height:25px;
	float:left;
	}	
.aniu{
	background:url(../../images/skin_0/login_pic/anniu.gif) no-repeat;
	width:86px;
	height:25px;
	line-height:25px;
	font-weight:bold;
	margin-left:10px;
	font-size:14px;
	filter:alpha(opacity=100);
	float:left;
	}
.aniu a{
	color:#000;
	text-decoration:none;
	}
/*.aniu a:hover{
	color:#FF8040;
	}	*/
/* footer */
.footer {
	overflow: hidden;
	margin: 0 auto;
	padding:0;
	margin:0;
	background: #f7f7f7;
	border-top: 1px solid #fff;
}

.footer-inner {
	width: 800px;
	height:53px;
	line-height:53px;
	overflow: visible;
	margin: 0 auto;
	color:#5FB0D5;
	font-size:12px;
	text-align:center;
	position: relative
}
	