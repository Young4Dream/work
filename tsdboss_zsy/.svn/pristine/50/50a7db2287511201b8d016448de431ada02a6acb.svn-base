package com.tsd.entity;

public class Bymx {

	private String hth;
	private String dh;
	private int dhid;
	private String feetype;
	private String thlx;
	private String thsc;
	private int ithsc;
	private double hfhj;
	private String style;
	private int bymonth;
	private int givenewfunc;
	private int pmode;
	private double totalfee;
	private double fee;
	private double giveheji;
	private int pkorder;
	
	public Bymx(){
		
	}
	
	public Bymx(String hth, String dh, int dhid, String feetype, String thlx,
			String thsc, int ithsc, double hfhj, String style, int bymonth,
			int givenewfunc, int pmode, double totalfee, double fee,
			double giveheji, int pkorder) {
		super();
		this.hth = hth;
		this.dh = dh;
		this.dhid = dhid;
		this.feetype = feetype;
		this.thlx = thlx;
		this.thsc = thsc;
		this.ithsc = ithsc;
		this.hfhj = hfhj;
		this.style = style;
		this.bymonth = bymonth;
		this.givenewfunc = givenewfunc;
		this.pmode = pmode;
		this.totalfee = totalfee;
		this.fee = fee;
		this.giveheji = giveheji;
		this.pkorder = pkorder;
	}
	
	public String getHth() {
		return hth;
	}

	public void setHth(String hth) {
		this.hth = hth;
	}

	public String getDh() {
		return dh;
	}

	public void setDh(String dh) {
		this.dh = dh;
	}

	public int getDhid() {
		return dhid;
	}

	public void setDhid(int dhid) {
		this.dhid = dhid;
	}

	public String getFeetype() {
		return feetype;
	}

	public void setFeetype(String feetype) {
		this.feetype = feetype;
	}

	public String getThlx() {
		return thlx;
	}

	public void setThlx(String thlx) {
		this.thlx = thlx;
	}

	public String getThsc() {
		return thsc;
	}

	public void setThsc(String thsc) {
		this.thsc = thsc;
	}

	public int getIthsc() {
		return ithsc;
	}

	public void setIthsc(int ithsc) {
		this.ithsc = ithsc;
	}

	public double getHfhj() {
		return hfhj;
	}

	public void setHfhj(double hfhj) {
		this.hfhj = hfhj;
	}

	public String getStyle() {
		return style;
	}

	public void setStyle(String style) {
		this.style = style;
	}

	public int getBymonth() {
		return bymonth;
	}

	public void setBymonth(int bymonth) {
		this.bymonth = bymonth;
	}

	public int getGivenewfunc() {
		return givenewfunc;
	}

	public void setGivenewfunc(int givenewfunc) {
		this.givenewfunc = givenewfunc;
	}

	public int getPmode() {
		return pmode;
	}

	public void setPmode(int pmode) {
		this.pmode = pmode;
	}

	public double getTotalfee() {
		return totalfee;
	}

	public void setTotalfee(double totalfee) {
		this.totalfee = totalfee;
	}

	public double getFee() {
		return fee;
	}

	public void setFee(double fee) {
		this.fee = fee;
	}

	public double getGiveheji() {
		return giveheji;
	}

	public void setGiveheji(double giveheji) {
		this.giveheji = giveheji;
	}

	public int getPkorder() {
		return pkorder;
	}

	public void setPkorder(int pkorder) {
		this.pkorder = pkorder;
	}

	
}
