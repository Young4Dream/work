//随机生成颜色
function randomClr()
{
        var nums=['0','3','6','9','C','F'];
        var clr='#';
        for(var i=0;i<6;i++)
        {
                var n=Math.round(Math.random()*10)%6;
                clr=clr + nums[n];                
        }
        return clr;
}
