@import url("../../../plug-in/extjs/resources/css/ext-skin0.css");
/*菜单样式*/
.tsd-tree-panel .x-panel-body,
.tsd-tree-panel .x-panel-bwrap{position:static !important; height: auto !important;}
.tsd-tree-panel .x-tree-node-indent{
	display: none;
}

.tsd-tree-panel .tree-node-level-1{
	margin:1px;
	margin-top:3px;
	padding-left:5px;
	background:  url('../../images/skin_0/tree_node_level_1_bg_1.jpg') repeat-x;
	height: 27px;
	line-height: 27px;
}

.tsd-tree-panel .tree-node-level-1 .x-tree-node-anchor span{
	color:#388BAB;
	font-weight:bold;
	font-size: 13px;
}
.tsd-tree-panel .tree-node-level-2{
	height: 22px;
	line-height: 22px;
	/* background:  none; */
	border-bottom:#e6f3fb 1px solid;
	margin: 1px 5px;
}
.tsd-tree-panel .tree-node-level-2 .x-tree-node-anchor span{
	color:#388BAB;
	font-weight:normal;
	font-size: 13px;
}
.tsd-tree-panel .tree-node-level-3{
	height: 22px;
	line-height: 22px;
	background-color: #DFF1FD;
	margin: 1px 21px;
}
.tsd-tree-panel .tree-node-level-3 .x-tree-node-anchor span{
	color:#000000;
	font-weight:normal;
	font-size: 13px;
}

.tsd-tree-panel .tree-node-level-4{
	height: 22px;
	line-height: 22px;
	background-color: #ECEEF0;
	margin: 0px 37px;
}
.tsd-tree-panel .tree-node-level-4 .x-tree-node-anchor span{
	color:#000000;
	font-weight:normal;
	font-size: 13px;
}

.tsd-tree-panel .x-tree-ec-icon, .tsd-tree-panel .x-tree-node-indent{
	visibility: hidden;
}
.tsd-tree-panel .tree-node-level-1 .x-tree-ec-icon{
	display: none;
}
.tsd-tree-panel .tree-node-level-2 .x-tree-elbow-end-plus, .tsd-tree-panel .tree-node-level-2 .x-tree-elbow-end-minus,
.tsd-tree-panel .tree-node-level-2 .x-tree-elbow-plus, .tsd-tree-panel .tree-node-level-2 .x-tree-elbow-minus,
.tsd-tree-panel .tree-node-level-3 .x-tree-elbow-end-plus, .tsd-tree-panel .tree-node-level-3 .x-tree-elbow-end-minus,
.tsd-tree-panel .tree-node-level-3 .x-tree-elbow-plus, .tsd-tree-panel .tree-node-level-3 .x-tree-elbow-minus,
.tsd-tree-panel .tree-node-level-4 .x-tree-ec-icon{
	visibility: visible;
}
.tsd-tree-panel .x-tree-insert-flag{
	border-top:2px dotted red !important;
}
.x-tree-node-el .x-tree-node-inline-icon{
	width: auto;
	height: auto;
	vertical-align:middle;
	background: none;
}

.tsd-tree-panel .tree-node-level-1-select{
	background-image:  url('../../images/skin_0/tree_node_level_1_bg_0.jpg');
}
.tsd-tree-panel .tree-node-level-1-select .x-tree-node-anchor span{
	color:#AC7204;
}
.tsd-tree-panel .tree-node-level-2-select{
	background-color: #73D0FC;
	border-bottom:1px solid #E6F3FB;
}
.tsd-tree-panel .tree-node-level-2-select .x-tree-node-anchor span{
	color:#ffffff;
	font-size:15px;font-weight:700;
}

.tsd-tree-panel .tree-node-level-3-select{
	background-color: #3399CC;
	border-bottom:1px solid #E6F3FB;
}
.tsd-tree-panel .tree-node-level-3-select .x-tree-node-anchor span{
	color:#ffffff;
	font-size:15px;font-weight:700;
}

.tsd-tree-panel .tree-node-level-4-select{
	background-color: #C8C8C8;
	border-bottom:1px solid #E5E5E5;
}