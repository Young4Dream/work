public class PackageGift {

	//主键
	private int id;
	
	//礼品分类 1、实物（如：自行车、U盘等）2、虚拟物品（如：充值卡、积分等）	3、其它
	private int gType;
	
	private String giftName;
	
	private String giftAll;
	
	private String gReason;
	
	private String gImg;
	
	private Date createTime;
	
	private Date updateTime;
	
	private String remark;
	
	private String gTypeTmp;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getGType() {
		return gType;
	}

	public void setGType(int type) {
		gType = type;
	}

	public String getGiftName() {
		return giftName;
	}

	public void setGiftName(String giftName) {
		this.giftName = giftName;
	}

	public String getGiftAll() {
		return giftAll;
	}

	public void setGiftAll(String giftAll) {
		this.giftAll = giftAll;
	}

	public String getGReason() {
		return gReason;
	}

	public void setGReason(String reason) {
		gReason = reason;
	}

	public String getGImg() {
		return gImg;
	}

	public void setGImg(String img) {
		gImg = img;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getGTypeTmp() {
		return gTypeTmp;
	}

	public void setGTypeTmp(String typeTmp) {
		gTypeTmp = typeTmp;
	}
}
